#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.add_widget:15
msgid "Canvas to add widget's canvas to. Can be 'before', 'after' or None for the default canvas."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_cancel_duration:1
msgid "Defines the duration of the animation when a swipe movement is not accepted. This is generally when the user does not make a large enough swipe. See :attr:`min_move`."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_cancel_duration:5
msgid ":attr:`anim_cancel_duration` is a :class:`~kivy.properties.NumericProperty` and defaults to 0.3."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_move_duration:1
msgid "Defines the duration of the Carousel animation between pages."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_move_duration:3
msgid ":attr:`anim_move_duration` is a :class:`~kivy.properties.NumericProperty` and defaults to 0.5."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_type:1
msgid "Type of animation to use while animating to the next/previous slide. This should be the name of an :class:`~kivy.animation.AnimationTransition` function."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.anim_type:5
msgid ":attr:`anim_type` is a :class:`~kivy.properties.StringProperty` and defaults to 'out_quad'."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.clear_widgets:1
msgid "Remove all (or the specified) :attr:`~Widget.children` of this widget. If the 'children' argument is specified, it should be a list (or filtered list) of children of the current widget."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.clear_widgets:5
msgid "The `children` argument can be used to specify the children you want to remove."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.current_slide:1
msgid "The currently shown slide."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.current_slide:3
msgid ":attr:`current_slide` is an :class:`~kivy.properties.AliasProperty`."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.current_slide:5
#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.next_slide:8
msgid "The property no longer exposes the slides container. It returns the widget you have added."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.direction:1
msgid "Specifies the direction in which the slides are ordered. This corresponds to the direction from which the user swipes to go from one slide to the next. It can be `right`, `left`, `top`, or `bottom`. For example, with the default value of `right`, the second slide is to the right of the first and the user would swipe from the right towards the left to get to the second slide."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.direction:9
msgid ":attr:`direction` is an :class:`~kivy.properties.OptionProperty` and defaults to 'right'."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.ignore_perpendicular_swipes:1
msgid "Ignore swipes on axis perpendicular to direction."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.ignore_perpendicular_swipes:3
msgid ":attr:`ignore_perpendicular_swipes` is a :class:`~kivy.properties.BooleanProperty` and defaults to False."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.index:1
msgid "Get/Set the current slide based on the index."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.index:3
msgid ":attr:`index` is an :class:`~kivy.properties.AliasProperty` and defaults to 0 (the first item)."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.load_next:1
msgid "Animate to the next slide."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.load_previous:1
msgid "Animate to the previous slide."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.load_slide:1
msgid "Animate to the slide that is passed as the argument."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.loop:1
msgid "Allow the Carousel to loop infinitely. If True, when the user tries to swipe beyond last page, it will return to the first. If False, it will remain on the last page."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.loop:5
msgid ":attr:`loop` is a :class:`~kivy.properties.BooleanProperty` and defaults to False."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.min_move:1
msgid "Defines the minimum distance to be covered before the touch is considered a swipe gesture and the Carousel content changed. This is a expressed as a fraction of the Carousel's width. If the movement doesn't reach this minimum value, the movement is cancelled and the content is restored to its original position."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.min_move:7
msgid ":attr:`min_move` is a :class:`~kivy.properties.NumericProperty` and defaults to 0.2."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.next_slide:1
msgid "The next slide in the Carousel. It is None if the current slide is the last slide in the Carousel. This ordering reflects the order in which the slides are added: their presentation varies according to the :attr:`direction` property."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.next_slide:6
msgid ":attr:`next_slide` is an :class:`~kivy.properties.AliasProperty`."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.on_touch_down:1
msgid "Receive a touch down event."
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.on_touch_down:7
msgid "`touch`: :class:`~kivy.input.motionevent.MotionEvent` class"
msgstr ""

#: ../../../../venv/lib/python3.6/site-packages/kivy/uix/carousel.py:docstring of kivy.uix.carousel.Carousel.on_touch_down:5
